  m_SharedData: {fileID: 11400000, guid: b3a7ada3e91db7a4c8cd806d4b8ae990, type: 2}
  m_Metadata:
    m_Items: []
  m_TableData:
  - m_Id: 6806298624
    m_Localized: "C\xF4 g\xE1i ch\u1EC9 c\xF2n gi\u1ECF kh\xF4ng, b\xE8n ng\u1ED3i
      xu\u1ED1ng b\u01B0ng m\u1EB7t kh\xF3c hu hu. \n\n"
    m_Metadata:
      m_Items: []
  - m_Id: 5529966469120
    m_Localized: "B\u1EA5y gi\u1EDD B\u1EE5t \u0111ang ng\u1ED3i tr\xEAn t\xF2a sen,
      b\u1ED7ng nghe ti\u1EBFng kh\xF3c c\u1EE7a c\xF4 li\u1EC1n hi\u1EC7n xu\u1ED1ng
      h\u1ECFi:\n\n"
    m_Metadata:
      m_Items: []
  - m_Id: 5602645368832
    m_Localized: "\"V\xEC sao con kh\xF3c?\"\n\n"
    m_Metadata:
      m_Items: []
  - m_Id: 6309196849152
    m_Localized: "C\xF4 g\xE1i k\u1EC3 s\u1EF1 t\xECnh cho B\u1EE5t nghe. B\u1EE5t
      b\u1EA3o:\r\n\n"
    m_Metadata:
      m_Items: []
  - m_Id: 6527405514752
    m_Localized: "\"Th\xF4i con h\xE3y n\xEDn \u0111i! Con th\u1EED nh\xECn v\xE0o
      gi\u1ECF xem c\xF2n c\xF3 g\xEC n\u1EEFa kh\xF4ng?\"\n\n"
    m_Metadata:
      m_Items: []
  - m_Id: 7294900232192
    m_Localized: "\"Ch\u1EC9 c\xF2n m\u1ED9t con c\xE1 b\u1ED1ng\"\n\n"
    m_Metadata:
      m_Items: []
  - m_Id: 7835126587392
    m_Localized: "\"Con \u0111em con c\xE1 b\u1ED1ng \u1EA5y v\u1EC1 nh\xE0 th\u1EA3
      xu\u1ED1ng gi\u1EBFng m\xE0 nu\xF4i. M\u1ED7i b\u1EEFa, \u0111\xE1ng \u0103n
      ba b\xE1t th\xEC con \u0103n hai, c\xF2n m\u1ED9t \u0111em th\u1EA3 xu\u1ED1ng
      cho b\u1ED1ng. M\u1ED7i l\u1EA7n cho \u0103n, con nh\u1EDB g\u1ECDi nh\u01B0
      th\u1EBF n\xE0y:\n\n"
    m_Metadata:
      m_Items: []
  - m_Id: 10010326196224
    m_Localized: "B\u1ED1ng b\u1ED1ng, bang bang\r\nL\xEAn \u0103n c\u01A1m v\xE0ng
      c\u01A1m b\u1EA1c nh\xE0 ta,\r\nCh\u1EDB \u0103n c\u01A1m h\u1EA9m, ch\xE1o
      hoa nh\xE0 ng\u01B0\u1EDDi.\""
    m_Metadata:
      m_Items: []
  - m_Id: 10126214815744
    m_Localized: "B\u1EE5t c\xF2n n\xF3i th\xEAm:\n\n"
    m_Metadata:
      m_Items: []
  - m_Id: 10219026374656
    m_Localized: "\"Kh\xF4ng g\u1ECDi \u0111\xFAng nh\u01B0 th\u1EBF n\xF3 kh\xF4ng
      l\xEAn, con nh\u1EDB l\u1EA5y!\"\n\n"
    m_Metadata:
      m_Items: []
  - m_Id: 400333678305280
    m_Localized: "B\u1EA1n \u0111ang \u0111i tr\xEAn \u0111\u01B0\u1EDDng th\xEC
      g\u1EB7p m\u1ED9t \u0111\xE1m ma tr\u01A1i t\xFAm t\u1EE5m l\u1EA1i. Khi nh\xECn
      th\u1EA5y b\u1EA1n, ch\xFAng b\u1ED7ng x\u1EBFp th\xE0nh m\u1ED9t h\xE0ng,
      ch\u1EB7n l\u1ED1i \u0111i v\xE0 b\u1EAFt \u0111\u1EA7u c\u1EA5t l\xEAn m\u1ED9t
      b\xE0i \u0111\u1ED3ng dao m\xE0 b\u1EA1n ch\u01B0a bao gi\u1EDD nghe qua: "
    m_Metadata:
      m_Items: []
  - m_Id: 404358180102144
    m_Localized: "\"R\u1ED3ng r\u1EAFn l\xEAn m\xE2y\r\nC\xF3 c\xE1i c\xE2y l\xFAc
      l\u1EAFc\r\nH\u1ECFi th\u0103m th\u1EA7y thu\u1ED1c\r\nC\xF3 \u1EDF nh\xE0
      hay kh\xF4ng?\""
    m_Metadata:
      m_Items: []
  - m_Id: 407137892163584
    m_Localized: "Tr\u1EA3 l\u1EDDi ch\xFAng"
    m_Metadata:
      m_Items: []
  - m_Id: 407244804972544
    m_Localized: "Th\u1EF1c hi\u1EC7n k\u0129 thu\u1EADt l\u1ED9n ch\u1EE5c v\xF2ng
      tr\xEAn kh\xF4ng \u0111\u1EC3 v\u01B0\u1EE3t qua l\u0169 ma tr\u01A1i"
    m_Metadata:
      m_Items: []
  - m_Id: 408214314147840
    m_Localized: "C\u1ED1 b\u1EAFt b\xF3ng ma \u0111\u1EE9ng cu\u1ED1i c\xF9ng trong
      h\xE0ng"
    m_Metadata:
      m_Items: []
  - m_Id: 411578074652672
    m_Localized: "\u0110\xE1m ma tr\u01A1i im l\u1EB7ng khi b\u1EA1n b\u01B0\u1EDBc
      qua. Ch\u1EC9 m\u1ED9t l\xFAc sau, b\u1EA1n c\u1EA3m th\u1EA5y c\u01A1 th\u1EC3
      m\xECnh c\xF3 ch\xFAt thay \u0111\u1ED5i."
    m_Metadata:
      m_Items: []
  - m_Id: 412586968342528
    m_Localized: "\u0110\xE1m ma tr\u01A1i kh\u1EF1ng l\u1EA1i khi b\u1EA1n b\u1EAFt
      th\xE0nh c\xF4ng \u0111\u1EE9a cu\u1ED1i c\xF9ng trong h\xE0ng."
    m_Metadata:
      m_Items: []
  - m_Id: 412871052746752
    m_Localized: "\"\u0110\u1EE3i \u0111\xE3, b\u1ECDn em ch\u01B0a h\u1EC1 n\xF3i
      lu\u1EADt ch\u01A1i m\xE0!\""
    m_Metadata:
      m_Items: []
  - m_Id: 413313627316224
    m_Localized: "Khi nh\xECn khu\xF4n m\u1EB7t ng\u01A1 ng\xE1c c\u1EE7a b\u1EA1n,
      l\u0169 ma b\u1EADt ra ti\u1EBFng c\u01B0\u1EDDi."
    m_Metadata:
      m_Items: []
  - m_Id: 413831330258944
    m_Localized: "\"C\xE1ch ch\u01A1i \u0111\xF3 ch\u1EC9 d\xE0nh cho ng\u01B0\u1EDDi
      d\u01B0\u01A1ng th\xF4i! N\u1EBFu ch\u1ECB r\u1EA3nh th\xEC l\u1EA7n sau h\xE3y
      ch\u01A1i v\u1EDBi ch\xFAng em nh\xE9, ch\u1ECB s\u1EBD bi\u1EBFt lu\u1EADt
      n\u01A1i n\xE0y!\""
    m_Metadata:
      m_Items: []
  - m_Id: 415610113613824
    m_Localized: "Nghe c\xE2u tr\u1EA3 l\u1EDDi c\u1ED9c c\u1EB1n c\u1EE7a b\u1EA1n,
      \u0111\xE1m ma tr\u01A1i kh\u1EF1ng l\u1EA1i. R\u1ED3i nh\u01B0 hi\u1EC3u ra
      \u0111i\u1EC1u g\xEC \u0111\xF3, ch\xFAng t\u1EA3n ra."
    m_Metadata:
      m_Items: []
  - m_Id: 418209806147584
    m_Localized: "\"Xin l\u1ED7i v\xEC \u0111\xE3 ng\xE1ng \u0111\u01B0\u1EDDng \u1EA1!\""
    m_Metadata:
      m_Items: []
  - m_Id: 419312383811584
    m_Localized: "B\u1EA1n nhanh ch\xF3ng r\u1EDDi \u0111i. Trong tay l\xE0 m\xF3n
      qu\xE0 t\u1EA1 l\u1ED7i c\u1EE7a \u0111\xE1m ma tr\u01A1i."
    m_Metadata:
      m_Items: []
  - m_Id: 487420549926912
    m_Localized: "\"Ta nh\u1EDB c\u1EA3m gi\xE1c \u0111\u01B0\u1EE3c v\u1EABy v\xF9ng
      b\xEAn con s\xF4ng qu\xEA qu\xE1.\""
    m_Metadata:
      m_Items: []
  - m_Id: 487946758918144
    m_Localized: "\u0110\xF3 l\xE0 nh\u1EEFng g\xEC m\xE0 h\u1ED3n ma tr\u01B0\u1EDBc
      m\u1EB7t \u0111\xE3 th\u1ED1t ra khi b\u1EA1n t\u1EDBi g\u1EA7n."
    m_Metadata:
      m_Items: []
  - m_Id: 488081383493632
    m_Localized: "Th\u1EA5y b\u1EA1n c\xF3 v\u1EBB v\u1ED9i v\xE3, h\u1ED3n ma ti\u1EBFp
      t\u1EE5c c\u1EA5t gi\u1ECDng tr\u1EA7m bu\u1ED3n. "
    m_Metadata:
      m_Items: []
  - m_Id: 488325882056704
    m_Localized: "\"N\u1EBFu ng\u01B0\u01A1i kh\xF4ng phi\u1EC1n, xin h\xE3y l\u1EAFng
      nghe l\xE3o gi\xE0 n\xE0y k\u1EC3 chuy\u1EC7n ch\u1EE9?\""
    m_Metadata:
      m_Items: []
  - m_Id: 488611224752128
    m_Localized: "B\u1EA1n suy ngh\u0129 m\u1ED9t l\xFAc. D\xF9 g\xEC th\xEC th\u1EDDi
      gian c\u0169ng ch\u1EB3ng c\xF3 ngh\u0129a l\xED g\xEC \u1EDF n\u01A1i n\xE0y
      c\u1EA3, chi b\u1EB1ng ng\u1ED3i nghe m\u1ED9t h\u1ED3n ma lang thang k\u1EC3
      chuy\u1EC7n, bi\u1EBFt \u0111\xE2u l\u1EA1i h\u1ECDc \u0111\u01B0\u1EE3c \u0111i\u1EC1u
      hay. "
    m_Metadata:
      m_Items: []
  - m_Id: 488930428063744
    m_Localized: "\"V\u1EADy ng\u01B0\u01A1i mu\u1ED1n nghe v\u1EC1 chuy\u1EC7n tr\xEAn
      n\xFAi, d\u01B0\u1EDBi s\xF4ng, hay b\xEAn bi\u1EC3n?\""
    m_Metadata:
      m_Items: []
  - m_Id: 489706110062592
    m_Localized: "M\u1ED9t c\xE2u chuy\u1EC7n li\xEAn quan t\u1EDBi n\xFAi c\xF3
      l\u1EBD s\u1EBD ph\xF9 h\u1EE3p v\u1EDBi m\xECnh"
    m_Metadata:
      m_Items: []
  - m_Id: 489869926993920
    m_Localized: "S\xF4ng \u01B0, c\u0169ng kh\xF4ng t\u1EC7"
    m_Metadata:
      m_Items: []
  - m_Id: 489944711434240
    m_Localized: "H\u1ED3n ma n\xE0y \u0111\xE3 t\u1EEBng ra bi\u1EC3n, ch\u1EAFc
      s\u1EBD c\xF3 chuy\u1EC7n hay \u0111\xE2y"
    m_Metadata:
      m_Items: []
  - m_Id: 490197338558464
    m_Localized: "\"H\u1ED3i c\xF2n s\u1ED1ng, ta t\u1EEBng \u0111\xE1nh hai con
      tr\xE2u \u0111i\xEAn ch\u1EC9 b\u1EB1ng m\u1ED9t c\xE1i g\u1EADy g\u1ED7...\"
      H\u1ED3n ma k\u1EC3 l\u1EA1i v\u1EDBi gi\u1ECDng b\u1ED3i h\u1ED3i"
    m_Metadata:
      m_Items: []
  - m_Id: 490607075921920
    m_Localized: "\"H\u1ED3i c\xF2n s\u1ED1ng, ta t\u1EEBng \u0111\u01B0\u1EE3c leo
      l\xEAn m\xECnh c\xE1 th\u1EA7n...\" H\u1ED3n ma k\u1EC3 l\u1EA1i v\u1EDBi gi\u1ECDng
      b\u1ED3i h\u1ED3i"
    m_Metadata:
      m_Items: []
  - m_Id: 490884239724544
    m_Localized: "\"H\u1ED3i c\xF2n s\u1ED1ng, m\u1ED9t thi\u1EBFu n\u1EEF n\u01B0\u1EDBc
      ngo\xE0i \u0111\xE3 v\xEC ta m\xE0 quy\xEAn sinh...\" H\u1ED3n ma k\u1EC3 l\u1EA1i
      v\u1EDBi gi\u1ECDng b\u1ED3i h\u1ED3i. H\xF3a ra b\xEAn bi\u1EC3n c\u1EE7a
      h\u1ED3n ma ngh\u0129a l\xE0 n\u01B0\u1EDBc ngo\xE0i"
    m_Metadata:
      m_Items: []
  - m_Id: 496395618095104
    m_Localized: "B\u1EA1n ch\xECm \u0111\u1EAFm trong c\xE2u chuy\u1EC7n do h\u1ED3n
      ma nh\u1EDB nh\xE0 k\u1EC3. Ch\u1EB3ng m\u1EA5y ch\u1ED1c, b\u1EA1n c\u1EA3m
      th\u1EA5y m\xECnh \u0111\xE3 h\u1ECDc \u0111\u01B0\u1EE3c th\u1EE9 g\xEC \u0111\xF3
      m\u1EDBi."
    m_Metadata:
      m_Items: []
